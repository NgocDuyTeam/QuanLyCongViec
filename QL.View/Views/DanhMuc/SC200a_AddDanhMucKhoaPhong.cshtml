
@{
    ViewBag.Title = "SC200a_AddDanhMucKhoaPhong";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Thêm mới Khoa Phòng</h4>
<div class="row content-all-ql" data-ng-controller="SC200DanhMucKhoaPhongCtrl" ng-cloak>
    <div class="row col-lg-12" style="margin-top:15px">
        <div style="width: 60%; height: 500px">
            <div>
                <div class="row col-lg-12">
                    <label class="row col-lg-6" for="inputEmail3" style="float:left;">Mã Khoa Phòng</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" required name="ma" ng-model="KhoaPhong.Ma">

                    </div>
                </div>
                <div class="row col-lg-12">
                    <label for="inputEmail3" class="row col-lg-6" style="float:left;">Tên Khoa Phòng</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" required name="ten" ng-model="KhoaPhong.Ten">

                    </div>
                </div>
                <div class="row col-lg-12">
                    <label for="inputEmail3" class="row col-lg-6" style="float:left;">Ghi Chú</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" ng-model="KhoaPhong.GhiChu">
                    </div>
                </div>
            </div>
          
            <div class="row col-lg-6" style="margin-top:5px;">

                <button type="submit" class="btn btn-default" style="float:left;" ng-click="huy()">Hủy</button>
                <button type="submit" style="margin-left:15px; float:right;" ng-disabled="isDisabled" class="btn btn-success pull-right" ng-click="addKhoaPhong()">Lưu</button>
            </div><!-- /.box-footer -->
       
        </div>
    </div>
</div>
