
@{
    ViewBag.Title = "SC202_DanhMucCanBo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Danh Mục Cán Bộ</h2>
<div class="row content-all-ql" data-ng-controller="SC202DanhMucCanBoCtrl" ng-cloak>
    <div class="row col-lg-12" style="margin-top:15px">
        <div style="width: 70%; height: 500px">
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th class="text-center">STT</th>
                        <th class="text-center">Ngày Tạo</th>
                        <th class="text-center">ID</th>
                        <th class="text-center">Họ và Tên</th>
                        <th class="text-center">Tài Khoản</th>
                        <th class="text-center">Mật Khẩu</th>
                        <th class="text-center">Role</th>
                        <th class="text-center">Tên Khoa</th>
                    </tr>
                </thead>
                <tbody>
                    <tr data-ng-repeat="CB in DSCanBo">
                        <td class="text-center">{{$index+1}}</td>
                        <td class="text-center">{{NgayTao | date: 'dd/MM/yyyy'}}</td>
                        <td class="text-center">{{CB.Id}}</td>
                        <td class="text-center">{{CB.HoVaTen}}</td>
                        <td class="text-center">{{CB.UserName}}</td>
                        <td class="text-center">{{CB.PassWord}}</td>
                        <td class="text-center">{{CB.Role}}</td>
                        <td class="text-center">{{CB.TenKhoa}}</td>
                  
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
