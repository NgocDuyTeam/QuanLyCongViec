
@{
    ViewBag.Title = "SC_201DanhMucCongViec";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Danh Sách Công Viêc</h4>
<div class="row content-all-ql" data-ng-controller="SC201DanhMucCongViecCtrl" ng-cloak>
    <div class="row col-lg-12" style="margin-top:15px">
        <div style="width: 70%; height: 500px">
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th class="text-center">STT</th>
                        <th class="text-center">Ngày Tạo</th>
                        <th class="text-center">Tên Khoa Phòng</th>
                        <th class="text-center">Ghi Chú</th>
                    </tr>
                </thead>
                <tbody>
                    <tr data-ng-repeat="CV in DSCongViec">
                        <td class="text-center">{{$index+1}}</td>
                        <td class="text-center">{{NgayTao | date: 'dd/MM/yyyy'}}</td>
                        <td class="text-center">{{CV.TenCongViec}}</td>
                        <td class="text-center">{{CV.GhiChu}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
